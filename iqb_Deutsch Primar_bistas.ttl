        <dedf4d28-c405-4b30-9f0f-e0df1ebe0a43> ;
    skos:notation "2.5" ;
    skos:prefLabel "Texte formulieren "@de .

<f908e500-16b4-4bf3-9d48-d7f9d4f13494> a skos:Concept ;
