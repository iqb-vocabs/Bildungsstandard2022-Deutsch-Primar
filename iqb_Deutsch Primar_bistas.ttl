    skos:notation "5.2" ;
    skos:prefLabel "Sprachliche Strukturen untersuchen und nutzen"@de .

<ca2563b4-6f1c-4962-9bee-e7c79350676c> a skos:Concept ;
    skos:broader <48405c01-791d-416f-bb15-764c5aa0db7f> ;
